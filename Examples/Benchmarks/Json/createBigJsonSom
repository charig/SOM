#!/usr/bin/env python2.7
homeDir = '/Users/guidochari/'
projectDir = homeDir + '/Documents/Projects/TruffleMATE'
benchmarkDir = 'Examples/Benchmarks/Json'
bigJsonFilename = 'AllSets-x.json'
bigJson = open (projectDir + '/' + benchmarkDir + '/JsonBig.som', 'w')
copyOfbigJson = open (projectDir + '/' + benchmarkDir + '/JsonBigTemplate.som', 'r')
dataFile = open (projectDir + '/' + benchmarkDir + '/' + bigJsonFilename, 'r')
bigJson.write(copyOfbigJson.read(191))
data = dataFile.read()
data = data.replace("\\'","")
data = data.replace("\\\\","")
data = data.replace("'","")
bigJson.write(data)
bigJson.write(copyOfbigJson.read())