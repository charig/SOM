SeveralObjectsFieldRead = Benchmark (
    | pairs |
    
    oneTimeSetup = (
    	pairs := Array new: 15.
    	1 to: 15 do: [:i | pairs at: i put: ((Pair new) key: i)].
    )
    
    benchmark = (
    	| value i |
    	i := 1.
    	[i < 15] whileTrue: [value := (pairs at: i) key. i := i + 1].
    	"1 to: 14 do: [:i | value := (pairs at: i) key]."
    	^(pairs at: 15) key
    )
    
    verifyResult: result = (
      ^self assert: result equals: 15
    )
)    
