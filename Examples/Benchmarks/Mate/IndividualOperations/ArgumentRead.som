ArgumentRead = Benchmark (
    | object random iterValue |
    
    oneTimeSetup = (
    	random := Random initialize.
    	object := PairTest new.
    )
    
    benchmark = (
    	"iterValue := random next" 
    	^object testArgRead: 1.
    )
    
    verifyResult: result = (
      ^result <= 101
    )
)    
