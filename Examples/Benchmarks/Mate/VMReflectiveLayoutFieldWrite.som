VMReflectiveLayoutFieldWrite = FieldWrite (
    initialize = (
    	| environment |
    	super initialize.
    	environment := EnvironmentMO 
							operationalSemantics:  
							message: nil 
							layout: LayoutFieldWriteSemanticsMO new.
		object installEnvironment: environment.
    )
    
    verifyResult: result = (
      ^self assert: object key equals: 1
    )
)