BasicToDoMate = BasicToDo (
    iterationOne = (self compileSplit)
    
    compileSplit = (
    	| ast send target read |
    	ast := (BasicToDo>>#oneTimeSetup) compilation.
    	send := (ast messageSendsWithSelector: 'problemSize') first.
    	send dispatchChain activeSpecializations first firstInstance split. 
    )
)    
